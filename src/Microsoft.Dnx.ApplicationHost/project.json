    },
    "Microsoft.Dnx.Runtime.Internals": {
      "version": "1.0.0-rc1-final",
      "type": "build"
    },
    "Microsoft.Dnx.Runtime.Sources": {
      "version": "1.0.0-rc1-final",
      "type": "build"
    },
    "Microsoft.Extensions.CommandLineUtils.Sources": {
      "version": "1.0.0-rc1-final",
      "type": "build"
    }
  },
  "frameworks": {
    "dnx451": {},
    "dnxcore50": {
      "dependencies": {
        "System.Collections": "4.0.11-beta-23516",
